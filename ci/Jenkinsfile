pipeline {
    
    agent {
        label 'csaa-agent'
    }
    
    
    stages{
        stage("Application Artifact Build"){
            steps{
              container("jnlp"){
                 
                 
                  sh '''cd app-src
                  mvn clean package -DskipTests=true''' 
                  
              }
                
            }
        }
        stage("skope stage"){
            steps{
                container("skopeo"){
                    sh "skopeo --version"
                }
                
            }
        }
    }
}
